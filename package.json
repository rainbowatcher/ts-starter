{
    "name": "ts-starter",
    "type": "module",
    "version": "0.0.0",
    "packageManager": "pnpm@9.1.3",
    "description": "",
    "author": "rainbowatcher <rainbow-w@qq.com>",
    "license": "MIT",
    "homepage": "https://github.com/rainbowatcher/ts-starter#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rainbowatcher/ts-starter.git"
    },
    "bugs": {
        "url": "https://github.com/rainbowatcher/ts-starter/issues"
    },
    "keywords": [
        "ts-starter"
    ],
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pkgroll",
        "lint": "eslint .",
        "prepare": "husky",
        "changelog": "conventional-changelog -si CHANGELOG.md",
        "release": "bumpp -all -x 'pnpm changlog' && npm publish",
        "start": "jiti src/index.ts",
        "test": "vitest",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@rainbowatcher/eslint-config": "^0.7.1",
        "@rainbowatcher/eslint-config-ignore": "^0.7.1",
        "@rainbowatcher/eslint-config-js": "^0.7.1",
        "@rainbowatcher/eslint-config-json": "^0.7.1",
        "@rainbowatcher/eslint-config-md": "^0.7.1",
        "@rainbowatcher/eslint-config-prettier": "^0.7.1",
        "@rainbowatcher/eslint-config-ts": "^0.7.1",
        "@types/node": "^20.12.12",
        "bumpp": "^9.4.1",
        "commitlint": "^19.3.0",
        "conventional-changelog-cli": "^5.0.0",
        "eslint": "^9.3.0",
        "husky": "^9.0.11",
        "jiti": "^1.21.0",
        "lint-staged": "^15.2.5",
        "pkgroll": "^2.1.1",
        "typescript": "^5.4.5",
        "vitest": "^1.6.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue,json}": "eslint --fix"
    }
}
